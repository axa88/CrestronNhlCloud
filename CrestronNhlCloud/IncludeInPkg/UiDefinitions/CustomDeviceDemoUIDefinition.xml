<?xml version="1.0" encoding="UTF-8" ?>
<uidefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://prd-use-rad-assets.azurewebsites.net/ExtensionsSchemaDefinition_v1_0.xsd">
  <version ver="1.0" />
  <tile status="{TileStatus}" icon="{TileIcon}" secondaryicon="{TileSecondaryIcon}" action="command:TileActionRoom" navigation="show:LayoutSetup" showinhomepage="#true" showinroompage="#true"/>
  <layouts>
    <layout id="LayoutDefault" title="LayoutDefaultTitle" subtitle="LayoutDefaultSubTitle" isdefaultlayout="#true"></layout>
    <layout id="LayoutSetup" title="^MainPageTitle" subtitle="^MainPageSubTitle">
      <controls>
        <subheader id="SubheaderIdTeamSelector" label="^SubheaderLabelTeamSelector" />
        <selectorbutton id="SelectorButtonIdTeam" label="^SelectorButtonLabelTeam" value="{SelectorButtonValueTeamKey}" />
        <subheader id="SubheaderIdNotificationDelay" label="^SubheaderLabelNotificationDelay" />
        <toggleslider id="ToggleSliderIdNotificationDelay" label="{ToggleSliderLabelNotificationDelayKey}" secondarylabel="^ToggleSliderLabel2NotificationDelay" togglevalue="{ToggleSliderToggleNotificationDelayKey}" slidervalue="{ToggleSliderValueNotificationDelayKey}" />
      </controls>
    </layout>
  </layouts>
</uidefinition>